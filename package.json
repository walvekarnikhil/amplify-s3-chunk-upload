{
  "name": "amplify-s3-chunk-upload",
  "version": "5.0.1",
  "description": "A custom storage upload plugin for AWS Amplify. Instead of reading file completely in memory, it helps to read file chunk by chunk.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/walvekarnikhil/amplify-s3-chunk-upload"
  },
  "bugs": {
    "url": "https://github.com/walvekarnikhil/amplify-s3-chunk-upload/issues"
  },
  "homepage": "https://github.com/walvekarnikhil/amplify-s3-chunk-upload",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest"
  },
  "keywords": [
    "amplify",
    "AWS-S3",
    "Large file upload"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-amplify/core": "^5.0.14",
    "@aws-amplify/storage": "^5.0.14",
    "@aws-sdk/client-s3": "3.6.1",
    "@aws-sdk/s3-request-presigner": "3.6.1",
    "@aws-sdk/util-create-request": "3.6.1",
    "@aws-sdk/util-format-url": "3.6.1",
    "axios": "0.26.0",
    "events": "^3.1.0",
    "tslib": "^1.8.0"
  },
  "resolutions": {
    "@aws-sdk/fetch-http-handler": "3.6.1",
    "@aws-sdk/client-s3": "3.6.1"
  },
  "readme": "README.md",
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "pathRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$"
        },
        "tsConfig": {
          "lib": [
            "es5",
            "es2015",
            "dom",
            "esnext.asynciterable",
            "es2017.object"
          ],
          "allowJs": true
        }
      }
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "ts-jest",
        {
          "types": [
            "node",
            "jest"
          ]
        }
      ]
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost/",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "dist",
      "lib",
      "lib-esm"
    ]
  },
  "devDependencies": {
    "@jest/globals": "^25.5.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^8.9.5",
    "@types/sinon": "^7.5.1",
    "jest": "^25.5.0",
    "jest-config": "^25.5.0",
    "prettier": "^2.4.1",
    "ts-jest": "^25.x.x",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3"
  },
  "peerDependencies": {
    "@aws-amplify/storage": "^5.0.14"
  }
}
